<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>500</servlet-name>
  	<display-name>500</display-name>
  	<description></description>
  	<jsp-file>/500.jsp</jsp-file>
  </servlet>
  <servlet>
  	<servlet-name>DashBoardServlet</servlet-name>
  	<display-name>DashBoardServlet</display-name>
  	<description></description>
  	<servlet-class>excilys.formation.java.cbd.servlet.DashBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>500</servlet-name>
  	<url-pattern>/500</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DashBoardServlet</servlet-name>
  	<url-pattern>/DashBoardServlet</url-pattern>
  </servlet-mapping>
</web-app>
